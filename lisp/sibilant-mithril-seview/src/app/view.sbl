(def create-view (components)
  (#(model)
    ["form.pure-form.pure-form-aligned"
      ["fieldset"
        (components.air-temp.view model)
        (components.water-temp.view model)
        ["button.pure-button.pure-button-primary" "Save"]]
      ["span" "Saved:"]]
  ))

(set module 'exports {
  create-view create-view
})
